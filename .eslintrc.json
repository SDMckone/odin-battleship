{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "off",
    "no-continue": "off",
    "no-loop-func": "off",
    "no-alert": "off"
  }
}
